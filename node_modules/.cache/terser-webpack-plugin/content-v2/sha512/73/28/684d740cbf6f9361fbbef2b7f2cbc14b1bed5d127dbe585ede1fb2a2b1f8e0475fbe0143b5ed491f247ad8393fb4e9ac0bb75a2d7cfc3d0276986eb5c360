 seaweed\",/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"br\",{}),\"\\uC2F8\\uB2E4 (ssa da) - to wrap \",/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"br\",{}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"br\",{}),\"\\uC804\\uD1B5 (jun tong) - traditional\",/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"br\",{}),\"\\uC778\\uAE30 (in ki) - popular\",/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"br\",{}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"a\",{target:\"_blank\",href:\"https://craftlog.com/m/i/1127775=s1280=h960\",children:\"\\uC591\\uB150\\uCE58\\uD0A8 (yang yeum chi kin)\"}),\" - marinated/seasoned chicken\",/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"br\",{}),\"\\uACE0\\uAD6C\\uB9C8 (go gu ma) - sweet potato\",/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"br\",{}),\"\\uBC30\\uB2EC (bae dal) - delivery\",/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"br\",{}),\"\\uBB38\\uD654 (mun hwa) - culture\",/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"br\",{}),\"\\uD3B8\\uD558\\uB2E4 (pyeon ha da) - to be comfortable/easy \",/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"br\",{})]}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"div\",{className:\"row\",children:/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"div\",{className:\"col-12 spacer5\"})})]});}}]);return Ep7;}(react_default.a.Component);/* -------------------------Variables------------------------- */function Episode7_App(){return/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"div\",{children:/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(Episode7_Ep7,{})});}var Episode7_Episode7=function Episode7(){return/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"div\",{className:\"app\",children:/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(Episode7_App,{})});};/* harmony default export */ var Episodes_Episode7 = (Episode7_Episode7);\n// CONCATENATED MODULE: ./src/Episodes/Episode8.js\n/* ---------------------RESOURCES--------------------- */var Episode8_Ep8=/*#__PURE__*/function(_React$Component){Object(inherits[\"a\" /* default */])(Ep8,_React$Component);var 